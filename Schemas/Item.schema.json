    "type": "object",
    "properties": {
        "Id": {
            "type": "integer",
            "description": "llave primaria"
        },
        "IdProduct": {
            "type": "integer",
            "description": "relacion con el producto"
        },
        "Quantity": {
            "type": "integer",
